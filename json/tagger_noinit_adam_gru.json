{
  "vocabulary":{
	"min_count":1
  }
  "dataset_reader":{
    "type":"sequence_tagging",
    "word_tag_delimiter":"<Deli>",
    "token_indexers":{
      "tokens":{
        "type":"single_id",
        "lowercase_tokens":true
      },
      "token_characters":{
        "type":"characters"
      }
    }
  },
  "evaluate_on_test":true,
  "model":{
    "type":"tagger",
    "text_field_embedder":{
      "tokens":{
        "type":"embedding",
        "embedding_dim":100,
        "pretrained_file":"https://s3-us-west-2.amazonaws.com/allennlp/datasets/glove/glove.6B.100d.txt.gz",
        "trainable":true,
		"scale_grad_by_freq":false
      },
      "token_characters":{
        "type":"character_encoding",
        "embedding":{
          "embedding_dim":8
        },
        "encoder":{
          "type":"cnn",
          "embedding_dim":8,
          "num_filters":30,
          "ngram_filter_sizes":[
            5
          ]
        },
        "dropout":0.2
      }
    },
    "stacked_encoder":{
      "type":"gru",
      "input_size":130,
      "hidden_size":50,
      "num_layers":2,
      "dropout":0.2,
      "bidirectional":true
    }
  },
  "iterator":{
    "type":"basic",
    "batch_size":16
  },
  "trainer":{
    "num_epochs":100,
    "patience":5,
    "optimizer":{
      "type":"adam",
	  "lr":0.001
    },
    "learning_rate_scheduler":{
      "type":"reduce_on_plateau",
      "factor":0.1,
      "mode":"max",
      "patience":2
    },
    "grad_norm":1.0,
    "validation_metric":"+f1-measure-overall"
  }
}